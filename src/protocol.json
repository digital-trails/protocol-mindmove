{
  "theme_light": {
    "accent": "#007BFF",
    "text": "#424242",
    "primary": "#F5F5F5",
    "secondary": "#FFFFFF",
    "secondarybutton": "#EEEEEE"
  },
  "theme_dark": {
    "accent": "#66B2FF",
    "text": "#FFFFFF",
    "primary": "#121212",
    "secondary": "#202020",
    "secondarybutton": "#333333"
  },
  "start_flow": "flow://flows/survey1",
  "home": {
    "element": {
      "type": "list",
      "elements": [
        {
          "type": "alert",
          "title": "Mind Move",
          "message": "Link movement and mental health",
          "icon": "/assets/logo1.png"
        },
        {
          "type": "goals"
        },
        {
          "type": "sessions",
          "left": {
            "text": "{0} Sessions Completed",
            "icon": {
              "url": "/assets/completed.png",
              "tint": false
            }
          },
          "right": {
            "text": "Launch Session",
            "icon": {
              "url": "/assets/unlocked.png",
              "tint": false
            },
            "action": "flow://flows/doses"
          }
        },
        {
          "type": "button",
          "action": {
            "text": "Show Survey Modal",
            "action": "navmodal://Survey"
          }
        },
        {
          "type": "carousel",
          "actions": [
            {
              "text": "inputs!",
              "icon": "/assets/home_mindtrails.png",
              "action": "flow://flows/inputs",
              "backgroundcolor": "#1A206AFF",
              "markcompleted": true
            },
            {
              "text": "survey 1",
              "icon": "/assets/home_instructions.png",
              "action": "flow://flows/how%20was%20your%20day%3f",
              "backgroundcolor": "#1A00E05A",
              "markcompleted": true
            },
            {
              "text": "survey 2",
              "icon": "/assets/home_resources.png",
              "action": "flow://flows/survey2",
              "backgroundcolor": "#1A00C2FF",
              "markcompleted": true
            },
            {
              "text": "survey 3",
              "icon": "/assets/home_anxiety.png",
              "action": "flow://flows/survey3",
              "backgroundcolor": "#1A5C2FDA",
              "markcompleted": true
            }
          ]
        },
        {
          "type": "reports",
          "title": "Reports",
          "backgroundcolor": "#1A206AFF",
          "reports": [
            {
              "title": "survey 1",
              "action": "flow://flows/how%20was%20your%20day%3F",
              "report": true
            },
            {
              "title": "survey 2",
              "action": "flow://flows/survey2",
              "report": true
            }
          ]
        },
        {
          "type": "tiles",
          "actions": [
            {
              "text": "Video Flow",
              "icon": "/assets/home_mindtrails.png",
              "action": "flow://flows/video",
              "backgroundcolor": "#1A206AFF",
              "markcompleted": false
            },
            {
              "text": "inputs!",
              "icon": "/assets/home_mindtrails.png",
              "action": "flow://flows/inputs",
              "backgroundcolor": "#1A206AFF",
              "markcompleted": true
            },
            {
              "text": "survey 1",
              "icon": "/assets/home_instructions.png",
              "action": "flow://flows/how%20was%20your%20day%3F",
              "backgroundcolor": "#1A00E05A",
              "markcompleted": true
            },
            {
              "text": "survey 2",
              "icon": "/assets/home_resources.png",
              "action": "flow://flows/survey2",
              "backgroundcolor": "#1A00C2FF",
              "markcompleted": true
            }
          ]
        }
      ]
    }
  },
  "menu": [
    {
      "text": "Home",
      "icon": "/assets/menu_home.png"
    }
  ],
  "settings": {
    "configs": "flow://flows/configs.json"
  },
  "triggers": [
    {
      "id": "trigger_1",
      "type": "Reminder",
      "threshold": "2.00:00:00",
      "action": {
        "type": "list",
        "listtype": "sequential",
        "actions": [
          {
            "type": "notification",
            "path": "flow://flows/random1",
            "title": "Reminder Alert!",
            "message": "We noticed you haven’t completed your random1"
          },
          {
            "type": "notification",
            "path": "flow://flows/survey2",
            "title": "Reminder Alert!",
            "message": "We noticed you haven’t completed your survey2"
          }
        ]
      }
    },
    {
      "type": "timing",
      "id": "trigger_2",
      "frequency": "00:05:00",
      "action": {
        "type": "notification",
        "survey": true,
        "path": "flow://flows/survey1",
        "title": "Survey Alert!",
        "expiry": "00:02:00",
        "message": "It's time for your survey1 survey"
      }
    },
    {
      "id": "trigger_3",
      "type": "timing",
      "frequency": "1",
      "time": "08:00",
      "action": {
        "type": "notification",
        "survey": true,
        "path": "flow://flows/survey2",
        "expiry": "11:00",
        "title": "Survey Alert!",
        "message": "It's time for your survey2 survey"
      }
    },
    {
      "id": "trigger_4",
      "type": "random",
      "days": 7,
      "choose": 3,
      "time": "${global:random_time,15:00}",
      "action": {
        "type": "list",
        "listtype": "random",
        "actions": [
          {
            "type": "notification",
            "survey": true,
            "path": "flow://flows/random1",
            "expiry": "18:00",
            "title": "Survey Alert!",
            "message": "It's time for your random1 survey"
          },
          {
            "type": "notification",
            "survey": true,
            "path": "flow://flows/random2",
            "expiry": "18:00",
            "title": "Survey Alert!",
            "message": "It's time for your random2 survey"
          },
          {
            "type": "notification",
            "survey": true,
            "path": "flow://flows/random3",
            "expiry": "18:00",
            "title": "Survey Alert!",
            "message": "It's time for your random3 survey"
          }
        ]
      }
    }
  ],
  "probes": [
    {
      "type": "Location",
      "optional": true,
      "interval": "00:00:01",
      "accuracy": 5
    }
  ],
  "devices": [
    {
      "type": "WisePill",
      "channel": "Webhook",
      "goals": [
        {
          "id": "wisepill_goal",
          "type": "binarypercentage",
          "field": "event",
          "days": 7,
          "bounds": [
            {
              "lower-bound": 0,
              "upper-bound": 0.6,
              "text": "You're missing several doses. Please try to take your medication more consistently.",
              "summary": "Low Adherence",
              "color": "#ff9187"
            },
            {
              "lower-bound": 0.6,
              "upper-bound": 0.8,
              "text": "You're taking most of your doses. A little more consistency will help.",
              "summary": "Moderate Adherence",
              "color": "#F5E050"
            },
            {
              "lower-bound": 0.8,
              "upper-bound": 1,
              "text": "You're consistently taking your medication. Keep it up!",
              "summary": "High Adherence",
              "color": "#65bf65"
            }
          ]
        }
      ]
    }
  ]
}
